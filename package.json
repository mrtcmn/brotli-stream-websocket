    "elysia",
    "websocket",
    "brotli",
    "gzip",
    "compression",
    "streaming",
    "real-time"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "elysia": "^1.3.4",
    "zstd-codec": "^0.1.5"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
